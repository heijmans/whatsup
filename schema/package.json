{
  "name": "whatsup-schema",
  "author": "Jan Heijmans",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@types/express": "^4.16.0",
    "@types/jsonwebtoken": "^7.2.8",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.3.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^3.11.2",
    "@types/node": "^10.12.1",
    "js-yaml": "^3.12.0",
    "nodemon": "^1.18.5",
    "openapi3-ts": "^1.0.3",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.8.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.4"
  },
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "ok": "yarn pq && yarn lint",
    "pq": "yarn pretty-quick",
    "prettier": "prettier --write 'src/**/*.ts' 'src/**/*.js*' 'src/**/*.yaml' '*.js*'",
    "run": "ts-node src/convert.ts",
    "start": "nodemon -w src -e ts,yaml -x 'yarn run run'",
    "to_backend": "cp src/api/api-types.ts src/api/*-controller.ts ../backend/src/api/ && cp src/lib/api-controller-helpers.ts ../backend/src/lib/"
  },
  "prettier": {
    "arrowParens": "always",
    "printWidth": 100,
    "trailingComma": "all"
  }
}
